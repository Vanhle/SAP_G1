<mvc:View
    controllerName="overviewdashboard.projectdashboard.controller.OverviewDashboard"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:layout="sap.ui.layout"
>
    <Page
        id="page"
        
        class="sapUiContentPadding"
    >
        <content>
            <layout:Grid
                id="_IDGenGrid1"
                width="100%"
            >
                <layout:content>
                    <Panel
                        id="_IDGenPanel"
                        width="100%"
                    >
                        <layoutData>
                            <layout:GridData
                                id="_IDGenGridData"
                                span="L6 M6 S12"
                            />
                        </layoutData>
                        <VBox id="_IDGenVBox">
                            <Button
                                id="_IDGenButton"
                                text="View Details"
                                press="onViewDetail"
                            />
                            <viz:VizFrame
                                id="idVizFrameColumn"
                                uiConfig="{applicationSet:'fiori'}"
                                vizType="column"
                                width="100%"
                                height="400px"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        id="_IDGenFlattenedDataset"
                                        data="{dataModel>/data}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                id="_IDGenDimensionDefinition"
                                                name="Day"
                                                value="{day}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                id="_IDGenMeasureDefinition"
                                                name="Quantity"
                                                value="{quantity}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem1"
                                        uid="valueAxis"
                                        type="Measure"
                                        values="Quantity"
                                    />
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem2"
                                        uid="categoryAxis"
                                        type="Dimension"
                                        values="Day"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </VBox>
                    </Panel>
                    <Panel
                        id="_IDGenPanelHorizontal"
                        width="100%"
                    >
                        <layoutData>
                            <layout:GridData
                                id="_IDGenGridData1"
                                span="L3 M3 S12"
                            />
                        </layoutData>
                        <VBox id="_IDGenVBoxHorizontal">
                            <Button
                                id="_IDGenButton3"
                                text="View Details"
                                press="onViewDetail"
                            />
                            <viz:VizFrame
                                id="idVizFrameHorizontal"
                                uiConfig="{applicationSet:'fiori'}"
                                vizType="bar"
                                width="100%"
                                height="400px"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        id="_IDGenFlattenedDatasetHorizontal"
                                        data="{severityModel>/data}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                id="_IDGenDimensionDefinitionHorizontal"
                                                name="Severity"
                                                value="{severity}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                id="_IDGenMeasureDefinitionHorizontal"
                                                name="Total"
                                                value="{total}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItemHorizontal1"
                                        uid="valueAxis"
                                        type="Measure"
                                        values="Total"
                                    />
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItemHorizontal2"
                                        uid="categoryAxis"
                                        type="Dimension"
                                        values="Severity"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </VBox>
                    </Panel>
                    <Panel
                        id="_IDGenPanel1"
                        width="100%"
                    >
                        <layoutData>
                            <layout:GridData
                                id="_IDGenGridData2"
                                span="L3 M3 S12"
                            />
                        </layoutData>
                        <VBox id="_IDGenVBox1">
                            <Button
                                id="_IDGenButton1"
                                text="View Details"
                                press="onViewDetail"
                            />
                            <viz:VizFrame
                                id="idVizFramePie"
                                uiConfig="{applicationSet:'fiori'}"
                                vizType="pie"
                                width="100%"
                                height="400px"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        id="_IDGenFlattenedDataset1"
                                        data="{logDataModel>/logData}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                id="_IDGenDimensionDefinition1"
                                                name="Log Type"
                                                value="{logType}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                id="_IDGenMeasureDefinition1"
                                                name="Count"
                                                value="{count}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem3"
                                        uid="size"
                                        type="Measure"
                                        values="Count"
                                    />
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem4"
                                        uid="color"
                                        type="Dimension"
                                        values="Log Type"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </VBox>
                    </Panel>
                </layout:content>
            </layout:Grid>
            <layout:Grid
                id="_IDGenGrid2"
                width="100%"
            >
                <layout:content>
                    <Panel
                        id="_IDGenPanel2"
                        width="100%"
                    >
                        <layoutData>
                            <layout:GridData
                                id="_IDGenGridData23"
                                span="L8 M8 S12"
                            />
                        </layoutData>
                        <VBox id="_IDGenVBox2">
                            <Button
                                id="_IDGenButton2"
                                text="View Details"
                                press="onViewDetail"
                            />
                            <viz:VizFrame
                                id="eventTimeVizFrame"
                                uiConfig="{applicationSet:'fiori'}"
                                vizType="line"
                                width="100%"
                                height="400px"
                            >
                                <viz:dataset>
                                    <viz.data:FlattenedDataset
                                        id="_IDGenFlattenedDataset2"
                                        data="{eventTimeModel>/data}"
                                    >
                                        <viz.data:dimensions>
                                            <viz.data:DimensionDefinition
                                                id="_IDGenDimensionDefinition2"
                                                name="Time"
                                                value="{eventTimeModel>time}"
                                            />
                                        </viz.data:dimensions>
                                        <viz.data:measures>
                                            <viz.data:MeasureDefinition
                                                id="_IDGenMeasureDefinition2"
                                                name="Quantity"
                                                value="{eventTimeModel>quantity}"
                                            />
                                        </viz.data:measures>
                                    </viz.data:FlattenedDataset>
                                </viz:dataset>
                                <viz:feeds>
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem5"
                                        uid="valueAxis"
                                        type="Measure"
                                        values="Quantity"
                                    />
                                    <viz.feeds:FeedItem
                                        id="_IDGenFeedItem6"
                                        uid="categoryAxis"
                                        type="Dimension"
                                        values="Time"
                                    />
                                </viz:feeds>
                            </viz:VizFrame>
                        </VBox>
                    </Panel>
                </layout:content>
            </layout:Grid>
        </content>
    </Page>
</mvc:View>
